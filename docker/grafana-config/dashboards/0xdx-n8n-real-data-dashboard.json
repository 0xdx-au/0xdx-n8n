          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT \n  DATE_TRUNC('hour', started_at) as time,\n  COUNT(*) as \"Executions per Hour\"\nFROM execution_entity \nWHERE started_at >= NOW() - INTERVAL '24 hours'\nGROUP BY DATE_TRUNC('hour', started_at)\nORDER BY time;",
